<Application x:Class="ServiceHost.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Application.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="PaletteDarkest"  Color="#9BA8AB"/>
            <SolidColorBrush x:Key="PaletteDarker"   Color="#4A5C6A"/>
            <SolidColorBrush x:Key="PaletteDark"     Color="#4A5C6A"/>
            <SolidColorBrush x:Key="PaletteMedium"   Color="#4A5C6A"/>
            <SolidColorBrush x:Key="PaletteLight"    Color="#9BA8AB"/>
            <SolidColorBrush x:Key="PaletteLightest" Color="#CCD0CF"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
    
    




